<?js
var data = obj;
var self = this;
var typeSignature = '';

if (data.type && data.type.names) {
    data.type.names.forEach(function (name) {
        typeSignature += '<span class="type-signature type ' + name.toLowerCase() + '">' + self.linkto(name, self.htmlsafe(name)) + '</span> ';
    });
}
?>
<div class="panel panel-default">
    <div class="panel-heading" id="<?js= id ?>"><?js= data.attribs + (data.scope === 'static' ? longname : name)  + typeSignature ?></div>
    <div class="panel-body">
        <?js if (data.summary) { ?>
            <div class="well well-sm summary">
                <?js= summary ?>
            </div>
        <?js } ?>

        <?js if (data.description) { ?>
            <div class="well well-sm">
                <?js= data.description ?>
            </div>
        <?js } ?>

        <?js= this.partial('details.tmpl', data) ?>

        <?js if (data.examples && examples.length) { ?>
            <h5>Example<?js= examples.length > 1? 's':'' ?></h5>
            <?js= this.partial('examples.tmpl', examples) ?>
        <?js } ?>
    </div>
</div>
